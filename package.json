{
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "prettier": "3.3.3",
    "prettier-plugin-jinja-template": "1.5.0"
  },
  "prettier": {
    "plugins": ["prettier-plugin-jinja-template"],
    "htmlWhitespaceSensitivity": "ignore",
    "overrides": [
      {
        "files": ["*.html"],
        "options": {
          "parser": "jinja-template"
        }
      }
    ]
  }
}
